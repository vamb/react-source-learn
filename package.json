    "eslint-config-fbjs": "^1.1.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-no-for-of-loops": "^1.0.0",
